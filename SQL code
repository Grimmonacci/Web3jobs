-- USE: SELECT * from jobs; 
-- to get the list

--Select * from jobs order by activity_level, company_name ASC;--
-- To get from most active platform -- 


CREATE TABLE jobs (
  ref_id INT PRIMARY KEY AUTO_INCREMENT ,
  company_name VARCHAR(40) UNIQUE,
  twitter_handle VARCHAR(40),
  url VARCHAR(40),
  activity_level VARCHAR(15)
);
drop table jobs;

ALTER TABLE jobs
MODIFY url VARCHAR(120);

